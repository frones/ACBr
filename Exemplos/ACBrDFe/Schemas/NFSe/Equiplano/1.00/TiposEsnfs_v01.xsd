	<xsd:simpleType name="tsNrCep">
		<xsd:annotation>
			<xsd:documentation>Número do CEP</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9]{8}" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="tsNrTelefone">
		<xsd:annotation>
			<xsd:documentation>Telefone</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1" />
			<xsd:maxLength value="20" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="tsValor">
		<xsd:annotation>
			<xsd:documentation>Valor - 18 dígitos, sendo até 2 para casas decimais</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:decimal">
			<xsd:totalDigits value="18" />
			<xsd:minInclusive value="0" />
			<xsd:pattern value="((\d){1,18})|((\d){1,16}\.(\d){1,2})|((\d){0,16}\.(\d){1,2})" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="tsPercentual">
		<xsd:annotation>
			<xsd:documentation>percentual - 5 dígitos, sendo até 4 para casas decimais</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:decimal">
			<xsd:totalDigits value="5" />
			<xsd:minInclusive value="0" />
			<xsd:pattern value="0\.[0-9]{0,4}|[0-9]{1}[0-9]{0,4}(\.[0-9]{0,4})?" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="tsDsDocumentoEstrangeiro">
