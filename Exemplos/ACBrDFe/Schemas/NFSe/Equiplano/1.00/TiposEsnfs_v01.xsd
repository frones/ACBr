	<!-- Tipo Composto -->
	<xsd:complexType name="tcServicoAtualizacao">
		<xsd:annotation>
			<xsd:documentation>Estrutura do serviço para atualização</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="item" type="tipos:tcServicoAeitem" minOccurs="1" maxOccurs="1" />
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="tcServicoAeitem">
		<xsd:annotation>
			<xsd:documentation>Estrutura do serviço AE item</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
   		<xsd:element name="nrItem" type="xsd:integer" minOccurs="1" maxOccurs="1" />
   		<xsd:element name="dsItem" type="xsd:string" minOccurs="0" maxOccurs="1" />
		<xsd:element name="isInformarDeducao" type="tipos:tsSimNao" minOccurs="0" maxOccurs="1" />
   		<xsd:element name="isTomadorObrigatorio" type="tipos:tsSimNao" minOccurs="0" maxOccurs="1" />
   		<xsd:element name="tpRetencao" type="tipos:tsTpRetencao"  minOccurs="0" maxOccurs="1" />
   		<xsd:element name="vlAliquota" type="tipos:tsValor" minOccurs="0" maxOccurs="1" />
   		<xsd:element name="subItemList" minOccurs="0" maxOccurs="1">
   			<xsd:complexType>
   				<xsd:sequence>
   					<xsd:element name="subItem" type="tipos:tcServicoAeSubItem" minOccurs="1" maxOccurs="unbounded"/>
   				</xsd:sequence>
   			</xsd:complexType>
   		</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
		
	<xsd:complexType name="tcServicoAeSubItem">
		<xsd:annotation>
			<xsd:documentation>Estrutura do serviço AE sub item</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="nrSubItem" type="xsd:integer" minOccurs="1" maxOccurs="1"/>
