<?xml version="1.0" encoding="utf-8"?>
<xsd:schema targetNamespace="http://www.equiplano.com.br/esnfs/tipos" xmlns:tipos="http://www.equiplano.com.br/esnfs/tipos" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:import namespace="http://www.w3.org/2000/09/xmldsig#" schemaLocation="xmldsig-core-schema_v01.xsd" />

	<!-- Tipo Simples-->
	<xsd:simpleType name="tsTpRetencao">
		<xsd:annotation>
			<xsd:documentation>Define critérios para o serviço subitem sobre a retenção de ISS</xsd:documentation>
			<xsd:documentation>1 - Permite</xsd:documentation>
			<xsd:documentation>2 - Não Permite</xsd:documentation>
			<xsd:documentation>3 - Exige</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-3]{1}" />
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="tsCnpj">
		<xsd:annotation>
			<xsd:documentation>Número do CNPJ</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9]{14}" />
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="tsTpIss">
		<xsd:annotation>
			<xsd:documentation>Tipo do ISS</xsd:documentation>
			<xsd:documentation>1 - Não contribui</xsd:documentation>
			<xsd:documentation>2 - Declarado</xsd:documentation>
			<xsd:documentation>3 - Fixo</xsd:documentation>
			<xsd:documentation>4 - Estimado</xsd:documentation>
			<xsd:documentation>5 - Arbitrado</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:integer">
			<xsd:pattern value="[0-5]{1}" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="tsNrInscricaoMunicipal">
		<xsd:annotation>
			<xsd:documentation>Tipo padrão referente a inscrição municipal.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:integer">
			<xsd:pattern value="[0-9]{1,20}" />
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="tsNmContato">
		<xsd:annotation>
			<xsd:documentation>Nome contato</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="2" />
			<xsd:maxLength value="60" />
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="tsNrDocumentoContador">
		<xsd:annotation>
			<xsd:documentation>Número do documento do contador</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="0" />
			<xsd:maxLength value="20" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="tsCdAutenticacao">
		<xsd:annotation>
	    	<xsd:documentation>Código de autenticação da Nfs-e</xsd:documentation>
	    </xsd:annotation>
	    <xsd:restriction base="xsd:string">
	    	<xsd:minLength value="8" />
	      	<xsd:maxLength value="100" />
	    </xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="tsCdMensagemAlerta">
		<xsd:annotation>
	    	<xsd:documentation>Código de mensagem de retorno do serviço</xsd:documentation>
	    </xsd:annotation>
	    <xsd:restriction base="xsd:string">
	    	<xsd:minLength value="1" />
	      	<xsd:maxLength value="4" />
	    </xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="tsDsMensagemAlerta">
		<xsd:annotation>
	      <xsd:documentation>Descrição da mensagem de retorno do serviço</xsd:documentation>
	    </xsd:annotation>
	    <xsd:restriction base="xsd:string">
	      <xsd:minLength value="1" />
	      <xsd:maxLength value="512" />
	    </xsd:restriction>
	  </xsd:simpleType>

	<xsd:simpleType name="tsDsComplemento">
		<xsd:annotation>
			<xsd:documentation>Descrição do complemento do endereço</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="0" />
			<xsd:maxLength value="40" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="tsStLoteRps">
		<xsd:annotation>
			<xsd:documentation>Situação do lote de RPS</xsd:documentation>
			<xsd:documentation>1 - Aguardando processamento</xsd:documentation>
			<xsd:documentation>2 - Não Processado, lote com erro</xsd:documentation>
			<xsd:documentation>3 - Processado com sucesso</xsd:documentation>
			<xsd:documentation>4 - Processado com avisos</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:int">
			<xsd:pattern value="[0-4]{1}" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="tsSimNao">
		<xsd:annotation>
			<xsd:documentation>Indicador de Sim ou Não</xsd:documentation>
			<xsd:documentation>0 = Não Enumerado</xsd:documentation>
			<xsd:documentation>1 = Sim</xsd:documentation>
			<xsd:documentation>2 = Não</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:int">
			<xsd:pattern value="[0-2]{1}" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="tsStRps">
		<xsd:annotation>
			<xsd:documentation>Indicador da situação do RPS</xsd:documentation>
			<xsd:documentation>1 - Converter</xsd:documentation>
			<xsd:documentation>2 - Converter e cancelar</xsd:documentation>
			<xsd:documentation>3 - Cancelar RPS</xsd:documentation>
			<xsd:documentation>4 - Rps subtituto</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:int">
			<xsd:pattern value="[1-4]{1}" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="tsTpTributacao">
		<xsd:annotation>
			<xsd:documentation>Indicador do tipo de tributação</xsd:documentation>
			<xsd:documentation>1 - Tributado no município</xsd:documentation>
			<xsd:documentation>2 - Tributado em outro município</xsd:documentation>
			<xsd:documentation>3 - Isento/Imune</xsd:documentation>
			<xsd:documentation>4 - Suspenso/Decisão judicial</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:int">
			<xsd:pattern value="[1-4]{1}" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="tsTpDocumento">
		<xsd:annotation>
			<xsd:documentation>Indicador do tipo do documento</xsd:documentation>
			<xsd:documentation>1 - CPF</xsd:documentation>
			<xsd:documentation>2 - CNPJ</xsd:documentation>
			<xsd:documentation>3 - Estrangeiro</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:int">
			<xsd:pattern value="[1-3]{1}" />
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="tsIdEntidade">
		<xsd:annotation>
			<xsd:documentation>Código identificador da entidade onde foi prestado o serviço</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:integer" />
	</xsd:simpleType>

	<xsd:simpleType name="tsNmTomador">
		<xsd:annotation>
			<xsd:documentation>Nome do tomador</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="0" />
			<xsd:maxLength value="80" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="tsEmail">
		<xsd:annotation>
			<xsd:documentation>E-mail</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="0" />
			<xsd:maxLength value="80" />
			<xsd:pattern value="[\w\-_]+(\.[\w\-_]+)*@(([\w\-]{2,63}\.)+[A-Za-z]{2,6}|\[\d{1,3}(\.\d{1,3}){3}\])" />
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="tsNrCidadeIbge">
		<xsd:annotation>
			<xsd:documentation>Código cidade IBGE</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:integer">
			<xsd:pattern value="[0-9]{1,7}" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="tsNrInscricaoEstadual">
