	<xsd:simpleType name="tsNmCidade">
		<xsd:annotation>
			<xsd:documentation>Nome cidade</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1" />
			<xsd:maxLength value="30" />
