  CabMsg, DadosMsg, DadosSenha: AnsiString): AnsiString;
begin
 Result := CabMsg +#13#10+
           '<?xml version="1.0" encoding="ISO-8859-1"?>' + #13#10+
           DadosMsg;
end;

